{
  "name": "adapt-it-library",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/adapt-it-library.es.js",
      "require": "./dist/adapt-it-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/adapt-it-library.umd.js",
  "module": "./dist/adapt-it-library.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.1.37",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "0.0.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JeanLiima/adapt-it-library"
  },
  "keywords": [],
  "author": "Jean Lima",
  "bugs": {
    "url": "https://github.com/JeanLiima/adapt-it-library/issues"
  },
  "homepage": "https://github.com/JeanLiima/adapt-it-library#readme",
  "license": "MIT"
}
